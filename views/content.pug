doctype html
html(lang="pl")
	head
		meta(charset='UTF-8')
		title Logging to Google
		style
			include ./css/content.css
	body
		include ./header.pug
		div.container
			form(action='http://localhost:3000/auth/google', method='GET')
				p
					| First Name: 
					input(type='text', name='first_name')
				p
					| Last Name: 
					input(type='text', name='last_name')
				input.sending(type='submit', value='Submit')
			include ./footer.pug	 